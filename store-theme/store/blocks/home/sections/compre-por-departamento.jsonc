        "children": [
            "responsive-layout.desktop#compre-por-departamento",
            "responsive-layout.mobile#compre-por-departamento"
        ],
        "props": {
            "blockClass": [
                "home-compre-por-departamento"
            ],
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "experimentalHideEmptyCols": true
        }
    },
    "responsive-layout.desktop#compre-por-departamento": {
        "children": [
            "flex-layout.col#home-compre-por-departamento-item-1",
