        "children": [
            "slider-layout#slider-compre-por-departamento-mobile"
        ]
    },
    "slider-layout#slider-compre-por-departamento-mobile": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 3,
                "phone": 3
            },
            "autoplay": {
                "timeout": "5000",
                "stopOnHover": "true"
            },
            "infinite": true,
            "showNavigationArrows": "never",
            "showPaginationDots": "mobileOnly",
            "blockClass": [
                "carousel",
                "compre-por-departamento"
            ],
            "fullWidth": false
        },
        "children": [
            "flex-layout.col#home-compre-por-departamento-item-1",
            "flex-layout.col#home-compre-por-departamento-item-2",
            "flex-layout.col#home-compre-por-departamento-item-3",
